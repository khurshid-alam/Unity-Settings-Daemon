<?xml version="1.0"?>
<gconfschemafile>
  <schemalist>
    <schema>
      <key>/schemas/apps/gnome_settings_daemon/editor/sync_text_types</key>
      <applyto>/apps/gnome_settings_daemon/editor/sync_text_types</applyto>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
        <short>Sync text/plain and text/* handlers</short>
        <long>Set to True to keep the handlers for text/plain and text/* MIME types synchronized.</long>
      </locale>
    </schema>
  </schemalist>
</gconfschemafile>

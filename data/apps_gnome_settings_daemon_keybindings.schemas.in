	    <default>6</default>
	    <locale name="C">
	        <short>Volume step</short>
		<long>Volume step as percentage of volume.</long>
	    </locale>
	</schema>

        <schema>
            <key>/schemas/apps/gnome_settings_daemon/keybindings/volume_mute</key>

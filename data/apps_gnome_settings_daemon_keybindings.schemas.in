            <default>XF86AudioMute</default>
            <locale name="C">
                <short>Volume mute</short>
                <long>Volume mute's shortcut.</long>
            </locale>
        </schema>


        <schema>
            <key>/schemas/apps/gnome_settings_daemon/keybindings/volume_down</key>
